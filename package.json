{
  "name": "manageiq-galaxy",
  "version": "1.0.0",
  "description": "Simple application to showcase how to achieve universal rendering and routing with React and Express.",
  "main": "src/server.js",
  "repository": "https://github.com/miq-consumption/manageiq-galaxy-web.git",
  "scripts": {
    "start:react": "react-scripts start",
    "start": "react-scripts build && node src/server.js",
    "build": "react-scripts build && node src/server.js",
    "start-development": "cross-env NODE_ENV=development node_modules/.bin/babel-node --presets react,es2015 src/server.js",
    "start-dev": "npm run start-dev-hmr",
    "start-dev-single-page": "node_modules/.bin/http-server src/static",
    "start-dev-hmr": "node_modules/.bin/webpack-dev-server --progress --inline --hot --open"
  },
  "author": "Luciano Mammino",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "^4.0.0-beta.2",
    "patternfly": "^3.37.4",
    "patternfly-react": "^1.2.0",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^15.6.2",
    "react-github-login": "^1.0.3",
    "react-icons": "^2.2.7",
    "react-markdown": "^3.1.3",
    "react-router": "^2.6.1",
    "react-scripts": "^1.1.0"
  },
  "devDependencies": {}
}
